	public String getTranslation( String objeto, String field, String value )
	{
		String resp = null;
		try
		{
			if( value.equalsIgnoreCase( '--' ) )
				resp = '--';
			else
			{
				System.debug( 'getTranslation( ' + objeto + ', ' + field + ', ' + value + ' )' );
				System.debug( 'mapTraduccionesXObjeto: ' + mapTraduccionesXObjeto );
				Map<String,Map<String,String>> mapObjeto = mapTraduccionesXObjeto.get( objeto.toLowerCase() );
				System.debug( 'mapObjeto: ' + mapObjeto );
				Map<String,String> mapCampos = mapObjeto.get( field.toLowerCase() );
				System.debug( 'mapCampos: ' + mapCampos );
				resp = mapCampos.get( value.toLowerCase() );
				System.debug( 'RESP: ' + resp );
			}
		}
		catch( System.exception e )
		{
			resp = null;
		}
		
		return resp;
	}
